use_bpm 80
synth :piano

live_loop :low do
  play :a2
  sleep 0.5
  play :e3
  sleep 0.5
  play :a3
  sleep 1
  play :c3
  sleep 0.5
  play :e3
  sleep 0.5
  play :fs5
  sleep 1
end

live_loop :hi do
  play :fs5
  sleep 1
  play :c5
  sleep 0.5
  play :e5
  sleep 0.5
  play :e3
  sleep 0.5
  play :fs5
  sleep 1.5
end
